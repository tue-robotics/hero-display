{
  "name": "hero-display",
  "productName": "HERO display",
  "version": "1.0.0",
  "description": "App for display on HERO robot",
  "main": "build/electron/main.js",
  "author": {
    "name": "Matthijs van der Burgh",
    "email": "MatthijsBurgh@outlook.com",
    "url": "https://github.com/MatthijsBurgh/"
  },
  "contributors": [
    {
      "name": "Matthijs van der Burgh",
      "email": "MatthijsBurgh@outlook.com",
      "url": "https://github.com/MatthijsBurgh/"
    },
    {
      "name": "Rein Appeldoorn",
      "url": "https://github.com/reinzor/"
    }
  ],
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tue-robotics/hero-display.git"
  },
  "bugs": {
    "url": "https://github.com/tue-robotics/hero-display/issues"
  },
  "homepage": "https://github.com/tue-robotics/hero-display#readme",
  "keywords": [
    "electron",
    "electron-quick-start",
    "electron-app",
    "electron-vue",
    "electron-vite",
    "electron-builder",
    "vuejs",
    "vue3",
    "vitejs"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "vite dev",
    "format": "prettier \"{{electron,src,tests}/**,.}/*.{ts,js,json,yml,css,scss,md,mts,vue}\" --check",
    "format:fix": "npm run format -- --write",
    "lint": "eslint \"{{electron,src,tests}/**,.}/*.{ts,js,vue,json,yml}\"",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "rimraf -rf build dist && npm run format:fix && npm run lint:fix && npm run generate-icons",
    "build": "vue-tsc && vite build && electron-builder --publish=never",
    "generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "preview": "vite preview",
    "test": "playwright test",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "clean": "rimraf -rf build dist node_modules",
    "version": "npm run build",
    "patch": "npm version patch -m 'v%s' && npm run postpublish",
    "minor": "npm version minor -m 'v%s' && npm run postpublish",
    "major": "npm version major -m 'v%s' && npm run postpublish",
    "postpublish": "git push -u && git push --tags"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.3",
    "auto-ros": "^2.2.0",
    "hero-vue": "^1.2.3",
    "roslib": "^2.0.1",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "electron": "^40.1.0",
    "electron-builder": "^26.7.0",
    "electron-builder-sandbox-fix": "^1.1.1",
    "electron-devtools-installer": "^4.0.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^17.3.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "sass-embedded": "^1.93.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.3"
  }
}
