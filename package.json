{
  "name": "hero-display",
  "version": "0.4.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tue-robotics/hero-display.git"
  },
  "author": {
    "name": "Rein Appeldoorn",
    "email": "reinzor@gmail.com"
  },
  "contributors": [
    {
      "name": "Matthijs van der Burgh",
      "email": "MatthijsBurgh@outlook.com"
    }
  ],
  "description": "App for display on HERO robot",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "deps": "npm run deps:missing && npm run deps:extra",
    "deps:missing": "dependency-check . --missing",
    "deps:extra": "dependency-check . --extra --no-dev",
    "prebuild": "npm run build:clean",
    "build": "node .electron-vue/build.js && electron-builder --publish=never",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir --publish=never",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "postbuild": "npm run deps",
    "dev": "node .electron-vue/dev-runner.js localhost",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --mode=production ${PROGRESS:---progress} --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --mode=production ${PROGRESS:---progress} --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run e2e",
    "postinstall": "npm run lint:fix",
    "version": "npm run build",
    "patch": "npm version patch -m 'v%s' && npm run postpublish",
    "minor": "npm version minor -m 'v%s' && npm run postpublish",
    "major": "npm version major -m 'v%s' && npm run postpublish",
    "postpublish": "git push && git push --tags"
  },
  "build": {
    "productName": "hero-display",
    "appId": "com.hero-display.display-app",
    "artifactName": "hero-display.${ext}",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons",
      "category": "Utility",
      "target": "AppImage"
    }
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "eventemitter2": "^6.4.3",
    "jpeg-js": "^0.4.1",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "roslib": "^1.1.0",
    "vue": "^2.6.11",
    "vue-awesome": "^4.0.2",
    "vue-electron": "^1.0.6",
    "vue-typer": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "@babel/runtime-corejs2": "^7.10.3",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-istanbul": "^6.0.0",
    "cfonts": "^2.8.5",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "del": "^5.1.0",
    "dependency-check": "^4.1.0",
    "electron": "^8.0.0",
    "electron-builder": "^22.7.0",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.0.0",
    "eslint": "^7.3.1",
    "eslint-config-standard": "^14.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.0.1",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "require-dir": "^1.2.0",
    "sass-loader": "^8.0.2",
    "spectron": "^10.0.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "^4.1.0",
    "vue-devtools": "^5.1.3",
    "vue-eslint-parser": "^7.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "optionalDependencies": {
    "bufferutil": "4.0.1",
    "utf-8-validate": "^5.0.2"
  }
}
